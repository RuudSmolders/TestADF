{
	"name": "sub_ExecUSQLScript",
	"properties": {
		"activities": [
			{
				"name": "DataLakeAnalyticsU-SQL1",
				"type": "DataLakeAnalyticsU-SQL",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"scriptPath": "2016/sales.usql",
					"scriptLinkedService": {
						"referenceName": "AzureDataLakeStore1",
						"type": "LinkedServiceReference"
					},
					"parameters": {
						"sales_file": {
							"value": "@concat('2016/',pipeline().",
							"type": "Expression"
						},
						"lineitems_file": "@concat('2016/output/linteitems_file/'\n,activity('GetSubFolders').output.itemName\n,'/'\n,activity('GetFileList').output.childItems.name\n,'.txt')"
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDataLakeAnalytics1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_CSVSubFolder",
						"type": "DatasetReference",
						"parameters": {
							"Input_SubFolderName": {
								"value": "@variables('input_FileName')",
								"type": "Expression"
							}
						}
					},
					"fieldList": [],
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			}
		],
		"variables": {
			"input_FileName": {
				"type": "String"
			}
		},
		"annotations": []
	}
}