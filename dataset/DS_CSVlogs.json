{
	"name": "DS_CSVlogs",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SubFolderName": {
				"type": "string",
				"defaultValue": "@item."
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('2016\\',dataset().SubFolderName,'\\')",
					"type": "Expression"
				},
				"container": "adf-data"
			},
			"columnDelimiter": "\t",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}