											"type": "String"
										},
										"sink": {
											"name": "CustomerName",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "OrderLineID",
											"type": "String"
										},
										"sink": {
											"name": "OrderLineID",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "StockItemID",
											"type": "String"
										},
										"sink": {
											"name": "StockItemID",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "StockItemName",
											"type": "String"
										},
										"sink": {
											"name": "StockItemName",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "Quantity",
											"type": "String"
										},
										"sink": {
											"name": "Quantity",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "UnitPrice",
											"type": "String"
										},
										"sink": {
											"name": "UnitPrice",
											"type": "String"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_dqy",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_dqy",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_dqy')]",
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_dqy')]"
			]
		}
